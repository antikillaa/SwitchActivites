<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stanislavpeshkur.someactivities">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/main_activity_title">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AboutPage"
            android:label="@string/about_text_title" />
        <activity
            android:name=".BirthdayActivity"
            android:label="@string/birthday_text_title" />
        <activity
            android:name=".DataTransfer"
            android:label="@string/data_transfer_text_title" />
        <activity
            android:name=".DataTransferReceive"
            android:label="@string/data_transfer_receive_title" />
        <activity android:name=".WhoThief" />
        <activity android:name=".WhoThiefChoose"></activity>
    </application>

</manifest>